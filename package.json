{
  "name": "cookie-monsters",
  "version": "0.0.1",
  "description": "Buy and eat cookies please.",
  "main": "index.js",
  "engines": {
    "node": ">=6.7.0"
  },
  "scripts": {
    "test": "check-node-version --node '>= 6.7.0' && mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "check-node-version --node '>= 6.7.0' && mocha --compilers js:babel-register --watch app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "build": "check-node-version --node '>= 6.7.0' && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && webpack -w",
    "start[dev]": "check-node-version --node '>= 6.7.0' && nodemon server/start.js",
    "start[prod]": "check-node-version --node '>= 6.7.0' && node server/start.js",
    "start": "check-node-version --node '>= 6.7.0' && npm run start[prod]",
    "seed": "node db/seed.js",
    "postinstall": "npm run build && node db/seed.js",
    "heroku-prebuild": "heroku config:set NPM_CONFIG_PRODUCTION=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spmcbride1201/cookie-monsters.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Sean McBride, Evan DiGiambattista, and Rachel Bird",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spmcbride1201/cookie-monsters/issues"
  },
  "homepage": "https://github.com/spmcbride1201/cookie-monsters#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-preset-stage-2": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "cookie-session": "^2.0.0-alpha.1",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pg-native": "^1.10.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.6",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "debug": "^2.6.0",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.4.2",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
    "volleyball": "^1.4.1",
    "webpack": "^1.13.3"
  }
}
